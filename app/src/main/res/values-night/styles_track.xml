<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TrackName">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_md</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:fontWeight">400</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="TrackDescription">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_xs</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:fontWeight">400</item>
        <item name="android:maxLength">30</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="TrackDot">
        <item name="android:tint">@color/white</item>
        <item name="android:layout_marginHorizontal">5dp</item>
    </style>
    <style name="TrackMore">
        <item name="android:tint">@color/white</item>
        <item name="android:layout_marginHorizontal">@dimen/spacing_xs</item>
    </style>
</resources>